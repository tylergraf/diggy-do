<div class="page page-view-admin-kid-feed">
  <ul class="unstyled kids-wrapper">
    <li ng-repeat="kid in kids" class="kid-list-item">
    <span class="avatar avatar-{{kid.avatar.color}}">
      <i class="icon icon-{{kid.avatar.icon}}"></i>
      <span class="kid-name">{{kid.name}}</span>
    </span>
    <span class="kid-points">{{kid.points}} points</span> 
    </li>
  </ul>
</div>  
